{
  "name": "nodejs-sandbox",
  "version": "0.0.1",
  "description": "A test sandbox for nodejs",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonniespratley/nodejs-sandbox.git"
  },
  "scripts": {
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "docs": "esdoc -c esdoc.json",

    "mocha": "mocha src/**/*-spec.js",
    "clean": "rimraf .tmp ./docs ./coverage",
    "build": "npm run publish",
    "publish": "npm run docs",
    "start": "node src/app.js",
    "test": "istanbul cover node_modules/.bin/_mocha -- -R spec src/**/*-spec.js",
    "posttest": "npm run coveralls",
    "pretest": "npm run clean",
    "watch": "nodemon -w src src/app.js",
    "watch-test": "nodemon -w src/ --exec 'npm run test'"
  },
  "author": "Jonnie Spratley",
  "license": "ISC",
  "dependencies": {
    "args-list": "^0.3.3",
    "body-parser": "^1.14.2",
    "debug": "^2.2.0",
    "errorhandler": "^1.4.3",
    "express": "^4.13.4",
    "level": "^1.4.0",
    "level-sublevel": "^6.5.4",
    "leveldown": "^1.4.4",
    "levelup": "^1.3.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.6",
    "esdoc": "^0.4.4",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "nodemon": "^1.8.1",
    "rimraf": "^2.5.1",
    "supertest": "^1.1.0"
  }
}
