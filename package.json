{
  "name": "nodejs-sandbox",
  "version": "0.0.1",
  "description": "A test sandbox for nodejs",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonniespratley/nodejs-sandbox.git"
  },
  "engines": {
    "node": ">=5.5.0",
    "npm": ">=3.3.0"
  },
  "scripts": {
    "build": "npm run publish",
    "clean": "rimraf .tmp ./data ./db ./test-db ./coverage ./reports",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "docs": "esdoc -c esdoc.json",
    "mocha": "mocha src/**/*-spec.js",
    "pretest": "npm run clean",
    "posttest": "npm run coveralls",
    "publish": "npm run docs",
    "start": "node index.js",
    "test": "istanbul cover node_modules/.bin/_mocha -- -R spec src/**/*-spec.js",
    "watch": "nodemon -w src index.js",
    "watch-test": "nodemon -w src/ --exec 'npm run test'"
  },
  "author": "Jonnie Spratley",
  "license": "ISC",
  "dependencies": {
    "args-list": "^0.3.3",
    "body-parser": "^1.14.2",
    "connect-flash": "^0.1.1",
    "debug": "^2.2.0",
    "ejs": "^2.4.1",
    "errorhandler": "^1.4.3",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "express-validator": "^2.19.1",
    "fs-extra": "^0.26.5",
    "jfs": "^0.2.6",
    "level": "^1.4.0",
    "level-sublevel": "^6.5.4",
    "leveldown": "^1.4.4",
    "levelup": "^1.3.1",
    "lodash": "^4.5.1",
    "serve-static": "^1.10.2"
  },
  "devDependencies": {
    "coveralls": "^2.11.6",
    "esdoc": "^0.4.4",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-debug": "^2.1.2",
    "gulp-inject": "^3.0.0",
    "gulp-load-plugins": "^1.2.0",
    "gulp-rimraf": "^0.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-tslint": "^4.3.2",
    "gulp-typescript": "^2.11.0",
    "istanbul": "^0.4.2",
    "merge2": "^1.0.1",
    "mocha": "^2.4.5",
    "node-notifier": "^4.5.0",
    "nodemon": "^1.8.1",
    "plato": "^1.5.0",
    "plop": "^1.5.0",
    "rimraf": "^2.5.1",
    "supertest": "^1.1.0",
    "tslint": "^3.5.0"
  }
}
